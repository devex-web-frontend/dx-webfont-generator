/*
 * {{fontName}} icon font glyphs
 */

@font-face {
  font-family: "{{fontName}}";
  src:
    url("{{fontsPath}}{{fontName}}.woff") format("woff"),
    url("{{fontsPath}}{{fontName}}.ttf") format("truetype");
  font-weight: normal;
  font-style: normal;
}

.{{baseClass}} {
  font-family: {{fontName}};
font-style: normal;
font-weight: normal;
font-variant: normal;
line-height: 1;
text-transform: none;
speak: none;
-webkit-font-smoothing: antialiased;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
  content: "\\{{this}}";
}
{{/each}}
